{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "create-block/rika",
	"version": "0.1.0",
	"title": "Rika",
	"category": "widgets",
	"icon": "smiley",
	"description": "Rika Dynamic post block.",
	"supports": {
		"html": false
	},
	"attributes": {
		"postsPerPage": {
			"type": "string",
			"default": 5
		},
		"isVisibleFeaturedImage": {
			"type": "boolean",
			"default": true
		},
		"order": {
			"type": "string",
			"default": "desc"
		},
		"orderby": {
			"type": "string",
			"default": "date"
		},
		"categories": {
			"type": "object"
		}
	},
	"textdomain": "rika",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
